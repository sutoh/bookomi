<% if @error.present? %>
  <div id="error_explanation">
    <%= @error %>
  </div>
<% end %>
<section class="tag-caption">
  <div class="inner">
    <div id="content-header" class="result-word">
      <h1><%= @tag.try(:name) %></h1>

    </div>
  </div>
</section>
<div id="container">
  <% @tag.mangas.each do |manga| %>
    <%= render partial: 'shared/tweet_content', locals: { tweet: manga.tweet } %> 
  <% end %>
</div>
